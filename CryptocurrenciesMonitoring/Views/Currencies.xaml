<Page x:Class="CryptocurrenciesMonitoring.Currencies"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CryptocurrenciesMonitoring"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="900"
      Title="Currencies">

    <Grid Background="WhiteSmoke">
        <StackPanel Height="550" Width="900">
            <Label Content="All Currencies" HorizontalAlignment="Center"/>
            <ListBox x:Name="CurrenciesListBox" SelectionMode="Single" Height="525">
                
            </ListBox>
        </StackPanel>
    </Grid>
</Page>
